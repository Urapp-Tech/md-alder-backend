{
  "name": "fastify-boilerplate",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon ./server.js",
    "start:staging": "cross-env NODE_ENV=staging node ./server.js",
    "start:prod": "cross-env NODE_ENV=production node ./server.js",
    "migrate:latest": "cross-env NODE_ENV=development knex migrate:latest",
    "migrate:rollback": "cross-env NODE_ENV=development knex migrate:rollback",
    "migrate:make": "cross-env NODE_ENV=development knex migrate:make",
    "seed:make": "cross-env NODE_ENV=development knex seed:make",
    "seed:all": "cross-env NODE_ENV=development knex seed:run",
    "seed:run": "cross-env NODE_ENV=development knex seed:run --specific",
    "script:run": "cross-env NODE_ENV=development node db/script/run-script.js",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "imports": {
    "#configs/*": "./src/configs/*.js",
    "#models/*": "./src/models/*.js",
    "#plugins/*": "./src/plugins/*.js",
    "#utilities/*": "./src/utilities/*.js",
    "#src/*": "./src/*.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.722.0",
    "@fastify/cors": "^10.0.2",
    "@fastify/env": "^5.0.2",
    "@fastify/formbody": "^8.0.1",
    "@fastify/jwt": "^9.0.2",
    "@fastify/multipart": "^9.0.1",
    "@fastify/redis": "^7.0.1",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "github:AsadGG/fastify-swagger-ui",
    "@lukeed/ms": "^2.0.2",
    "@sendgrid/mail": "^8.1.4",
    "@sinclair/typebox": "^0.34.13",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "fastify-bcrypt": "^1.0.1",
    "fastify-plugin": "^5.0.1",
    "fastify-rabbitmq": "^3.1.0",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "nodemailer": "^6.10.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "pg": "^8.13.1",
    "uuid": "^11.0.4",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@inquirer/prompts": "^7.2.1",
    "chalk": "^5.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^15.14.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2"
  }
}
